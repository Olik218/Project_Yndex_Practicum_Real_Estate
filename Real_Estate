    "\n",
    "#для нагляности посчитаем медианы средней стоимости квартир по количеству комнат\n",
    "flat_price_room = flat.pivot_table(index = 'rooms', values = 'last_price', aggfunc = ['count', 'median'])\n",
    "flat_price_room.columns = ['count', 'median']\n",
    "#в исследовании используем те квартиры которых больше 30, тем самым откинем квартиры с большим количеством комнат, \n",
    "#таких квартир мало, но они могут исказить график\n",
    "flat_price_room.query('count > 30').plot(y = 'median', title = (' Влияние количества комнат на стоимость квартиры'))\n",
    "\n",
    "flat_price_room.query('count > 30').sort_values('median', ascending = False)\n",
    "\n",
    "flat['rooms'].corr(flat['last_price'])\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
