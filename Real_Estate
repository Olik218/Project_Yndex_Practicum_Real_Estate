   "source": [
    "#создадим функцию для категоризации квартир\n",
    "def type_float (row):\n",
    "    floor = row['floor']\n",
    "    floors_total = row['floors_total']\n",
    "    if floor==1:\n",
    "        return 'первый'\n",
    "    if floor==floors_total:\n",
    "        return 'последний'\n",
    "    return 'другой'\n",
    "\n",
    "#проверяем квартиры по  этажам с помощью функции  \n",
    "flat['floor_category'] = flat.apply(type_float, axis = 1)\n"
   ]
