    "city = flat.pivot_table(index = 'locality_name', values = 'price_meter', aggfunc=['count', 'mean'])\n",
    "city.columns = ['count', 'mean']\n",
    "city = city.sort_values('count', ascending = False).head(10)\n",
    "#расчитаем самую высокую стоимость\n",
    "print ('Город из 10ки с максимальной стоимстью жилья:')\n",
    "display (city[city['mean']==city['mean'].max()])\n",
    "#самая низкая стоимость\n",
    "print ('Город из 10ки с минимальной стоимстью жилья:')\n",
    "display (city[city['mean']==city['mean'].min()])\n",
    "\n",
    "city.plot(y='mean',style='o-', figsize=(15,6),title='Средние цены в том-10')"
